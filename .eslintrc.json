{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define:": "off",

    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
